<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="True">
    <uie:PropertyField binding-path="mainGesture" name="mainGesture" />
    <uie:PropertyField binding-path="secondaryGesture" name="secondaryGesture" />
    <uie:PropertyField binding-path="interactionType" name="interactableType" />
    <ui:VisualElement name="snapGroup" style="display: flex; align-items: stretch;">
        <ui:ListView binding-path="snapData" show-border="true" reorderable="true" virtualization-method="DynamicHeight" show-bound-collection-size="false" reorder-mode="Animated" fixed-item-height="0" show-add-remove-footer="true" name="snapDataList" style="margin-top: 10px; margin-bottom: 0; align-items: stretch;" />
        <ui:VisualElement name="editSpacer" style="align-items: center; margin-top: 10px; justify-content: space-around; flex-direction: column;">
            <ui:VisualElement name="editGroup" style="min-width: auto; min-height: auto; width: 204px;">
                <ui:Button text="Edit" display-tooltip-when-elided="true" name="editButton" style="border-bottom-left-radius: 0; border-bottom-right-radius: 0; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0);" />
                <ui:VisualElement name="editTools" style="align-items: center; border-left-width: 1px; border-right-width: 1px; border-top-width: 1px; border-bottom-width: 1px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-top-left-radius: 2px; border-bottom-left-radius: 2px; border-top-right-radius: 2px; border-bottom-right-radius: 2px; min-height: 50px; height: auto; justify-content: center;">
                    <ui:Button text="Flip" display-tooltip-when-elided="true" name="editFlip" />
                </ui:VisualElement>
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
